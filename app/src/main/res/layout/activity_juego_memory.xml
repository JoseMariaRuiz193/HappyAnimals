<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".JuegoMemoryActivity">

    <TextView
        android:id="@+id/tv_p1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_p2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="340dp"
        android:textColor="@color/black"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/question6"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question5"
        app:layout_constraintTop_toBottomOf="@+id/question2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButtonBack"
        android:layout_width="89dp"
        android:layout_height="22dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="botonback"
        android:onClick="returnVolver"
        android:scaleType="fitXY"
        android:src="@drawable/back"
        app:layout_constraintStart_toEndOf="@+id/question4"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/question7"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question6"
        app:layout_constraintTop_toBottomOf="@+id/question3"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question11"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question10"
        app:layout_constraintTop_toBottomOf="@+id/question7"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question3"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="12dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question12"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question11"
        app:layout_constraintTop_toBottomOf="@+id/question8"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question8"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question7"
        app:layout_constraintTop_toBottomOf="@+id/question4"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question4"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question3"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question10"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question9"
        app:layout_constraintTop_toBottomOf="@+id/question6"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/preguntasonido3"
        android:layout_width="424dp"
        android:layout_height="66dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="44dp"
        android:background="@android:color/transparent"
        android:onClick="recargaDePantalla"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/tryagain2"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageOk"
        android:layout_width="610dp"
        android:layout_height="160dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="108dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/congrats" />

    <ImageButton
        android:id="@+id/question2"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toEndOf="@+id/question1"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question5"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question1"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="12dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/question9"
        android:layout_width="83dp"
        android:layout_height="110dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cartajuego"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question5"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>